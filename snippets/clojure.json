{
  "spy": {
    "prefix": "spy",
    "body": [
      "(log/spy $1)"
    ],
    "description": "SPY ON SOMETHING"
  },
  "un-namespace": {
    "prefix": "nx",
    "body": [
      "(ns-unmap *ns* '$1)"
    ],
    "description": "un-namespace something"
  },
  "transaction": {
    "prefix": "tx",
    "body": [
      "(comment",
      "  (do",
      "    (require '[aclaimant.services.core.db.customer-schema.fns :as fns])",
      "      (fns/transaction-in-schema \"$1\" ($2))))"
    ],
    "description": "For that sweet transaction"
  },
  "pretty": {
    "prefix": "px",
    "body": [
      "(do",
      " (require '[clojure.edn :as edn])",
      " (edn/read-string (slurp \"$1.edn\")))"
    ],
    "description": "Pretty print what you saved in a file"
  },
  "comment": {
    "prefix": "cx",
    "body": [
      "(comment",
      "  $1)"
    ],
    "description": "A sick comment block"
  }
}
